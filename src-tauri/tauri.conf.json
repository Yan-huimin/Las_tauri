{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "demo_las",
  "version": "0.1.0",
  "identifier": "com.yhm.dev",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173"
  },
  "app": {
    "windows": [
      {
        "title": "demo_las",
        "resizable": true,
        "minWidth": 1280,
        "minHeight": 800,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
